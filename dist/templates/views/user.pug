extends ../layouts/sketchfab

block intro
	

block content
	if user
		#singleContainer
			h1#username= data.username.first + " " + data.username.last 
			p: a.linkID(href = '/') Back to main menu
			h1#title= data.audience.title
			#email= data.emails
		#loadingScreen
		#sketchContainer
			p.url= data.audience.url
			p.description= data.audience.description

	else
		p We have created a default Admin user for you with the email <strong>james.miller@momentumww.com</strong> and the password <strong>keystone</strong>.
		p
			a(href='/signin', style='margin-right: 10px').btn.btn-lg.btn-primary Sign in
			|  to use the Admin UI.

block js
	script(src='/js/bundle.min.js')